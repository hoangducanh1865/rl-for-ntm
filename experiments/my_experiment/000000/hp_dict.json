{
    "num_gpus": 1,
    "num_cpus": 0,
    "max_epochs": 1000,
    "frozen_embeddings": true,
    "data_set": "data/pickles/20newsgroups_mwl3",
    "vocab_size": 2000,
    "input_type": "bert",
    "embedding_dict": {
        "sbert_model": "all-MiniLM-L6-v2",
        "hugface_model": "sentence-transformers/all-MiniLM-L6-v2",
        "size": 384,
        "max_length": 256
    },
    "num_topics": 20,
    "inference_dropout": 0.2,
    "theta_dropout": 0.0,
    "hidden_layers": [
        128,
        128
    ],
    "activation": "gelu",
    "initialization": "normal",
    "normalization": "layer",
    "affine": false,
    "prior": "laplace",
    "trainable_prior": true,
    "kl_mult": 5.0,
    "entropy_mult": 0.0,
    "parameter_noise": false,
    "top_k": [
        10
    ],
    "loss_type": "rl",
    "lda_type": "prodlda",
    "theta_softmax": false,
    "learning_rate": 0.0003,
    "betas": [
        0.9,
        0.999
    ],
    "weight_decay": 0.01,
    "batch_size": 1024,
    "gradient_clip": 1.0,
    "lr_decay": false
}